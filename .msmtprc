defaults
auth on
tls on
auto_from on
logfile ~/.msmtp.log

account gmail
host smtp.gmail.com
port 587
tls_certcheck off
from johndoe@gmail.com
user john@gmail.com
passwordeval passwordeval

account gmx
host mail.gmx.net
port 587
tls_certcheck off
from johndoe@gmx.de
user johndoe@gmx.de
passwordeval "pass gmx"

account gmx2
host mail.gmx.net
port 587
tls_certcheck off
from johndoe2@gmx.de
user johndoe2@gmx.de
passwordeval "pass gmx2"
