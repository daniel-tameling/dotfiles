style "gtk-theme-config-panel" {
     bg[NORMAL] = "#0f1b29"
     bg[PRELIGHT]      = "#213d60"
#      bg[PRELIGHT] = shade(1.1,"#000000")
     bg[ACTIVE]        = "#072850"
#      bg[ACTIVE] = shade(0.9,"#000000")
     bg[SELECTED] = shade(0.97,"#000000")
     fg[NORMAL] = "#729fcf"
     fg[PRELIGHT] = "#729fcf"
     fg[SELECTED] = "#729fcf"
     fg[ACTIVE] = "#729fcf"
     text[NORMAL]      = "#5b9ee6"
}

